    <style name="button_text_16" parent="text_16_FFFFFF">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_marginStart">25dp</item>
        <item name="android:layout_marginEnd">25dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/color_FFFFFF</item>
        <item name="android:enabled">false</item>
    </style>

    <style name="edit_text_16" parent="text_16">
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textColorHint">@color/color_9E9E9E</item>
        <item name="android:backgroundTint">@color/color_DEDEDE</item>
    </style>

    <style name="edit_text_16_" parent="text_16">
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textColorHint">@color/color_9E9E9E</item>
        <item name="android:backgroundTint">@color/color_DEDEDE</item>
    </style>

    <style name="TabTextAppearance">
        <item name="android:textSize">@dimen/text_size_16</item>
    </style>

    <style name="Widget.Design.TextInputLayout" parent="AppTheme">
        <item name="hintTextAppearance">@style/AppTheme.TextFloatLabelAppearance</item>
        <item name="errorTextAppearance">@style/AppTheme.TextErrorAppearance</item>
        <item name="counterTextAppearance">@style/TextAppearance.Design.Counter</item>
        <item name="counterOverflowTextAppearance">@style/TextAppearance.Design.Counter.Overflow
        </item>
    </style>

    <style name="AppTheme.TextFloatLabelAppearance" parent="TextAppearance.Design.Hint">
        <!-- Floating label appearance here -->
        <item name="android:textColor">@color/color_9E9E9E</item>
        <item name="android:textSize">@dimen/text_size_12</item>
    </style>

    <style name="AppTheme.TextErrorAppearance" parent="TextAppearance.Design.Error">
        <!-- Error message appearance here -->
        <item name="android:textColor">@color/color_FF3A5C</item>
        <item name="android:textSize">@dimen/text_size_12</item>
    </style>

    <style name="CustomDialog" parent="Theme.AppCompat.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="SplashTheme" parent="AppTheme">
        <item name="android:windowBackground">@drawable/splash</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="CustomNavigation" parent="Widget.AppCompat.Toolbar.Button.Navigation">
        <item name="android:background">@null</item>
    </style>

    <style name="LoadingDialogTheme" parent="Theme.AppCompat.Dialog">
        <!--  边框  -->
        <item name="android:windowFrame">@null</item>
        <!--  是否浮现在activity之上  -->
        <item name="android:windowIsFloating">true</item>
        <!--  半透明  -->
        <item name="android:windowIsTranslucent">true</item>
        <!--  背景  -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!--  无标题  -->
        <item name="android:windowNoTitle">true</item>
        <!--  模糊  -->
        <item name="android:backgroundDimEnabled">true</item>
        <!--  颜色  -->
        <item name="colorAccent">@color/color_00A761</item>
    </style>

</resources>
